{
    "name": "chqthomas/approval-tests",
    "description": "Approval Tests Library for PHP",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.68"
    },
    "autoload": {
        "psr-4": {
            "ApprovalTests\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "phpunit": {
            "extensions": [
                "ApprovalTests\\PHPUnit\\ApprovalTestsExtension"
            ]
        }
    }
}
